@model IEnumerable<LeQuocTrinh_2011063970.Models.Course>
@{
    ViewBag.Title = "Home Page";
}

<div class="row">
    <ul class="course">
        @foreach (var course in Model)
         {
                <li>
                    <div class="date">
                        <div class="month">
                            @course.DateTime.ToString("MM");
                        </div>
                    </div>
                    <div class="details">
                        <span class="lecturer">
                            @course.Category.Name
                        </span>
                    </div>
                </li>
         }
    </ul>
</div>
